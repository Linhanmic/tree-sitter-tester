{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tester",
      "camelcase": "Tester",
      "title": "Tester",
      "scope": "source.tester",
      "file-types": [
        "tester"
      ],
      "injection-regex": "^tester$",
      "class-name": "TreeSitterTester"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "DSL for ECU test! Make test easy -_-",
    "authors": [
      {
        "name": "Linhanmic",
        "email": "Linhanmic@qq.com",
        "url": "https://linhanmic.github.io/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tester"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
